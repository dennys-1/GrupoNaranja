@model inicio.Models.Contacto

<form asp-controller="Contacto" asp-action="Create">
     <label asp-for="Nombre" style="color: white;"class="control-label"></label>
     <input asp-for="Nombre" class="form-control" />

     <label asp-for="Email"style="color: white;" class="control-label"></label>
     <input asp-for="Email" class="form-control" />

     <label asp-for="Celular" style="color: white;"class="control-label"></label>
     <input asp-for="Celular" class="form-control" maxlength="9" />

     <label asp-for="Detalles"style="color: white;" class="control-label"></label>
     <input asp-for="Detalles" class="form-control" />

    <div class="form-group">
        <button type="submit" class="btn btn-primary">Registrar</button> 
    </div>              
</form>
<h1>@ViewData["Message"]</h1>